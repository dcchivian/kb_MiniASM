{
    "ver": "0.0.1",
    "authors": [
        "umaganapathyswork"
    ],
    "contact": "help@kbase.us",
    "categories": ["active", "assembly", "communities"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [
        {
            "id": "read_libraries",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseFile.SingleEndLibrary", "KBaseAssembly.SingleEndLibrary"]
            }
        },
        {
            "id": "output_contigset_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["MiniASM.contigs"],
            "field_type": "text",
            "text_options": {
                "valid_ws_types" : [ "KBaseGenomeAnnotations.Assembly" ],
                "is_output_name":true
            }
        },
        {
            "id": "min_contig",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "300" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "int",
		        "min_int" : 0
            }
        },
        {
            "id": "pre_filter",
            "optional":true,
            "advanced":true,
            "allow_multiple":false,
            "default_values":["0"],
            "field_type" : "checkbox",
            "checkbox_options":{
                "checked_value": 1,
                "unchecked_value": 0
            }
        },
        {
            "id": "min_match_len",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "100" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "int",
		        "min_int" : 1
            }
        },
        {
            "id": "min_identity",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0.05" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "float",
		        "min_int" : 0
            }
        },
        {
            "id": "min_span",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "2000" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "int",
		        "min_int" : 1
            }
        },
        {
            "id": "min_coverage",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "3" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "int",
		        "min_int" : 1
            }
        },
        {
            "id": "min_overlap",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "2000" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "int",
		        "min_int" : 1
            }
        },
        {
            "id": "max_overhang_len",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "1000" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "int",
		        "min_int" : 0
            }
        },
        {
            "id": "min_match_ratio",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0.8" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "float",
		        "min_int" : 0
            }
        },
        {
            "id": "max_gap_diff",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "1000" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "int",
		        "min_int" : 0
            }
        },
        {
            "id": "max_dist_bubble",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "50000" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "int",
		        "min_int" : 0
            }
        },
        {
            "id": "small_unitig_threshold",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "4" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "int",
		        "min_int" : 0
            }
        },
        {
            "id": "rounds_short_overlap",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "3" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "int",
		        "min_int" : 0
            }
        },
        {
            "id": "max_overlap_ratio",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0.7" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "float",
		        "min_int" : 0
            }
        },
        {
            "id": "min_overlap_ratio",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0.5" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "float",
		        "min_int" : 0
            }
        },
        {
            "id": "aggr_overlap_ratio",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0.8" ],
            "field_type": "text",
            "text_options": {
                "validate_as" : "float",
		        "min_int" : 0
            }
        },
        {
            "id": "output_info",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": ["ug"],
            "field_type" : "dropdown",
            "dropdown_options":{
            "options": [
                {
                 "value": "bed",
                 "display": "bed"
                },
                {
                  "value": "paf",
                  "display": "paf"
                },
                {
                  "value": "sg",
                  "display": "sg"
                },
                {
                  "value": "ug",
                  "display": "ug"
                }
              ]
            }
        },
        {
            "id": "both_directions",
            "optional":true,
            "advanced":true,
            "allow_multiple":false,
            "default_values":["0"],
            "field_type" : "checkbox",
            "checkbox_options":{
                "checked_value": 1,
                "unchecked_value": 0
            }
        },
        {
            "id": "skip_1_pass",
            "optional":true,
            "advanced":true,
            "allow_multiple":false,
            "default_values":["0"],
            "field_type" : "checkbox",
            "checkbox_options":{
                "checked_value": 1,
                "unchecked_value": 0
            }
        },
        {
            "id": "skip_2_pass",
            "optional":true,
            "advanced":true,
            "allow_multiple":false,
            "default_values":["0"],
            "field_type" : "checkbox",
            "checkbox_options":{
                "checked_value": 1,
                "unchecked_value": 0
            }
        },
        {
            "id": "print_version",
            "optional":true,
            "advanced":true,
            "allow_multiple":false,
            "default_values":["0"],
            "field_type" : "checkbox",
            "checkbox_options":{
                "checked_value": 1,
                "unchecked_value": 0
            }
        }
    ],

    "parameter-groups": [
	   {
		"id": "preSelect_args",
		"parameters": ["pre_filter", "min_match_len", "min_identity", "min_span", "min_coverage"],
	        "optional": true,
           "advanced": true,
		"allow_multiple": false,
		"with_border": true
       },
        {
		"id": "overlap_args",
		"parameters": ["min_overlap", "max_overhang_len", "min_match_ratio"],
	        "optional": true,
           "advanced": true,
		"allow_multiple": false,
		"with_border": true
	   },
        {
		"id": "layout_args",
		"parameters": ["max_gap_diff", "max_dist_bubble", "small_unitig_threshold", "rounds_short_overlap",
                        "max_overlap_ratio", "min_overlap_ratio", "aggr_overlap_ratio"],
	        "optional": true,
           "advanced": true,
		"allow_multiple": false,
		"with_border": true
	   },{
		"id": "misc_args",
		"parameters": ["output_info", "both_directions", "skip_1_pass", "skip_2_pass", "print_version"],
	        "optional": true,
           "advanced": true,
		"allow_multiple": false,
		"with_border": true
	   }
    ],

   "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_MiniASM",
            "method": "run_MiniASM",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "read_libraries",
                    "target_property": "read_libraries",
                    "target_type_transform": "list<ref>"
                },
                {
                    "input_parameter": "output_contigset_name",
                    "target_property": "output_contigset_name"
                },
                {
                    "input_parameter": "min_contig",
                    "target_property": "min_contig"
                },
                {
                    "input_parameter": "preSelect_args",
                    "target_property": "preSelect_args"
                },
                {
                    "input_parameter": "overlap_args",
                    "target_property": "overlap_args"
                },
                {
                    "input_parameter": "layout_args",
                    "target_property": "layout_args"
                },
                {
                    "input_parameter": "misc_args",
                    "target_property": "misc_args"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "constant_value": "5",
                    "target_property": "report_window_line_height"
                },
                {
                    "service_method_output_path": [0],
                    "target_property": "MiniASM_result"
                },
                {
                    "input_parameter": "read_libraries",
                    "target_property": "input_read_libraries"
                },
                {
                    "input_parameter": "output_contigset_name",
                    "target_property": "input_output_contigset_name"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
